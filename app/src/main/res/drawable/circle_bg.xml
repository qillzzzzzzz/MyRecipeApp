<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Warna latar belakang lingkaran -->
    <solid android:color="@color/cooki_brown" />

    <!-- Ukuran atau batas luar (opsional) -->
    <stroke
        android:width="2dp"
        android:color="@color/cooki_brown_dark" />

    <!-- Padding di dalam shape (opsional) -->
    <padding
        android:left="4dp"
        android:top="4dp"
        android:right="4dp"
        android:bottom="4dp" />
</shape>
